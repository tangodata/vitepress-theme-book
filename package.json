{
  "name": "vitepress-theme-book",
  "displayName": "Vitepress Book Enhanced",
  "version": "1.0.3",
  "description": "Simple vitepress theme for ebooks",
  "author": "tangodata",
  "license": "MIT",
  "keywords": [
    "vitepress",
    "theme"
  ],
  "xtype": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./theme": "./src/theme.js",
    "./register": "./src/core/register.js",
    "./config": "./src/vitepress/baseConfig.js",
    "./*": "./src/*"
  },
  "files": [
    "src",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangodata/vitepress-theme-book.git"
  },
  "bugs": {
    "url": "https://github.com/tangodata/vitepress-theme-book/issues"
  },
  "homepage": "https://github.com/tangodata/vitepress-theme-book#readme",
  "publisher": "tangodata",
  "scripts": {
    "dev": "vitepress dev docs --host 0.0.0.0 ",
    "build": "vitepress build docs"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "pinia": "^2.0.14"
  },
  "devDependencies": {
    "@originjs/vite-plugin-content": "^1.0.1",
    "@tailwindcss/typography": "^0.5.2",
    "@vueuse/core": "^8.5.0",
    "@vueuse/head": "^0.7.6",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.15.1",
    "feed": "^4.2.2",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.1",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-regexp": "^0.4.0",
    "postcss": "^8.4.14",
    "pug": "^3.0.2",
    "sass": "^1.52.1",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.9",
    "vite-plugin-compress": "^2.1.1",
    "vite-plugin-radar": "^0.4.2",
    "vitepress": "^0.22.4",
    "vue": "^3.2.36",
    "vue-sfc-rollup": "^4.1.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react",
        "vite"
      ]
    },
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "semver-regex@<3.1.3": ">=3.1.3",
      "svgo": ">=2.0.0",
      "uuid": ">=7.0.0"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "_unused": {
    "rd": "^2.0.1",
    "stylus": "^0.55.0",
    "timeago.js": "^4.0.2"
  }
}
